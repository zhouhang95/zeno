QLineEdit[cssClass="dark"] {
    border: 1px solid #252525;
    padding: 0 4px;
    background: #252525;
    selection-background-color: #4B9EF4;
	font-size: 9pt;;
    font-weight: 500;
    color: rgba(255,255,255,0.40);
}

QLineEdit[cssClass="dark"]:focus
{
    color: rgba(255,255,255,1.0);
}

